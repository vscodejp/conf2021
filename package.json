{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss,sass}\" --ignore-path .gitignore",
    "lint": "eslint ./ --ext ts,tsx",
    "lint:fix": "eslint ./ --ext ts,tsx --fix",
    "test": "jest",
    "test-cover": "jest --coverage",
    "test:w": "npm run test-cover -- --watch"
  },
  "dependencies": {
    "@headlessui/react": "1.7.4",
    "i18next": "22.0.6",
    "i18next-browser-languagedetector": "7.0.1",
    "next": "12.3.4",
    "preact": "10.11.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "8.0.4",
    "react-player": "2.11.0"
  },
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@babel/runtime": "7.20.6",
    "@nekohack/eslint-config-react": "0.10.0",
    "@types/enzyme": "3.10.7",
    "@types/jest": "26.0.15",
    "@types/node": "18.11.10",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "@wojtekmaj/enzyme-adapter-react-17": "0.4.1",
    "enzyme": "3.11.0",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.0",
    "next-pwa": "5.6.0",
    "prettier": "2.8.0",
    "sass": "1.56.1",
    "ts-jest": "26.4.1",
    "typescript": "4.9.3"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.test.json"
      }
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mock__/style-mock.js",
      "\\.(css|scss|less)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "components/**/*.{ts,tsx}"
    ],
    "testMatch": [
      "**/__tests__/*.(ts|tsx)"
    ],
    "testPathIgnorePatterns": [
      "./.next/",
      "./node_modules/"
    ]
  }
}
